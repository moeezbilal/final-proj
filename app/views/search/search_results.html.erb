
 
<div id="indexSearch">

<div id="contact_title">Search</div>
<div class="inside_body">

<div class="results_count">
<p>
<b><%= @count %></b> results for<b> <%= @name %> </b>
</p>
</div>


<table>
  <% @users.each do |user| %>

  <div  class="result">  

<%if user.image.length !=0 %>
<%= image_tag user.image_url(:postpic).to_s , :class=>"floatL userPicture"%>
<%else%>
  <div class="floatL userPicture"></div>  
<%end%>


	</div>    
  	<div class="floatL userName">
    <%= user.first_name %> <%= user.last_name %>
    </div>

 	 <%= button_to "Connect", {action: "connect", id:user.id, remote: true, :method => :post}, {class: 'connectButton'} %>


   <div style="clear:both">




</div>
     
	
    </tr>
    
    

  <% end %>
</table>

</div>