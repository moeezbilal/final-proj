<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>

    
      
        <%= f.label :email ,:class=>"labels"%>
       
          <%= f.email_field :email, :class => "singin_input_field" %></br>
        
      


        <%= f.label :password ,:class=>"labels"%>
        

          <%= f.password_field :password , :class => "singin_input_field"%><span><a href="/users/password/new"> Forgot password?</a></span>
        
      
      
      <div >
        
          <%= f.submit "Sign in",:id=>"singin_btn"%>
          <% if devise_mapping.rememberable? -%>
        
          
            <%= f.check_box :remember_me %>
            <span>Remember me</span>
            
          
        
      <% end -%>
      </div>

      

    
    <% end %>



